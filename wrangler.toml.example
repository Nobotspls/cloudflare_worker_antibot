name = "nobotspls-worker"
main = "worker.js"
compatibility_date = "2024-09-01"

# Bind environment variables
[vars]
AFK_MODE   = "monitor"          # or "enforce"
AFK_API_KEY = "afk_xxx..."      # optional but recommended
API_BASE   = "https://nobotspls.com"

